<Window x:Class="WebApiClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebApiClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Width="400">
            <Label x:Name="Output"></Label>
            <DataGrid Height="100" x:Name="lstbxPackages"/>
            <TextBox x:Name="txtSeach" TextWrapping="Wrap" Text="TextBox" Width="120"/>
            <Button Content="Button" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
